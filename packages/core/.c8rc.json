{
  "all": true,
  "src": ["src"],
  "exclude": [
    "test/**",
    "benchmark/**",
    "dist/**",
    "**/*.d.ts",
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/index.ts",
    "**/types.ts",
    "src/types/**",
    "src/model/types.ts"
  ],
  "reporter": [
    "text",
    "html",
    "lcov"
  ],
  "check-coverage": true,
  "lines": 80,
  "functions": 75,
  "branches": 80,
  "statements": 80,
  "clean": true
}
