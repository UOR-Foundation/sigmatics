{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "sub96",
  "description": "Subtraction modulo 96 with optional overflow tracking",
  "type": "object",
  "properties": {
    "name": { "type": "string", "const": "sub96" },
    "version": { "type": "string", "pattern": "^1\\.0\\.0$" },
    "namespace": { "type": "string", "const": "stdlib.ring" },
    "compiled": {
      "type": "object",
      "properties": {
        "overflowMode": { "type": "string", "enum": ["drop", "track"] }
      },
      "required": ["overflowMode"]
    },
    "runtime": {
      "type": "object",
      "properties": {
        "a": { "type": "integer", "minimum": 0, "maximum": 95 },
        "b": { "type": "integer", "minimum": 0, "maximum": 95 }
      },
      "required": ["a", "b"]
    },
    "complexityHint": { "type": "string", "const": "C1" },
    "loweringHints": {
      "type": "object",
      "properties": {
        "prefer": { "type": "string", "const": "class" }
      }
    }
  },
  "required": ["name", "version", "namespace", "compiled", "runtime"]
}
